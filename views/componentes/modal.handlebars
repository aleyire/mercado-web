{{#each modal}}
<div class="modal" tabindex="-1">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title">Carrito</h5>
        <button id="modal-cerrar" type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body">
        <ul>
            <img src="/img/{{this}}.png" alt="">
        </ul>
      </div>
      <div class="modal-footer">
        <button id="btn-cerrar" type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cerrar</button>
      </div>
    </div>
  </div>
</div>
{{/each}}